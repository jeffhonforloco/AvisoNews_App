# API Configuration
EXPO_PUBLIC_API_URL=

# News APIs
# Get your free API keys from:
# NewsAPI: https://newsapi.org/register
EXPO_PUBLIC_NEWS_API_KEY=your_newsapi_key_here

# GNews: https://gnews.io
EXPO_PUBLIC_GNEWS_API_KEY=your_gnews_key_here

# NewsData.io: https://newsdata.io
EXPO_PUBLIC_NEWSDATA_API_KEY=your_newsdata_key_here

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_NEWSLETTER=true
EXPO_PUBLIC_USE_REAL_NEWS_APIS=true

# Push Notifications
EXPO_PUBLIC_PROJECT_ID=your-expo-project-id

# App Configuration
EXPO_PUBLIC_APP_NAME=AvisoNews
EXPO_PUBLIC_APP_VERSION=1.0.0
